{
	"name": "Test Extension",
	"version": "1.0",
	"manifest_version": 2,
	"content_scripts":[
		{
			"matches": ["*://*.schoology.com/grades/*"],
			"js": ["rungrades.js"],
			"css" : ["soorajschromeextension.css"]
		},
		{
			"matches": ["*://*.schoology.com/home*"],
			"js": ["runhome.js"],
			"css" : ["soorajschromeextension.css"]
		}
	],
	"background": {
		"matches": ["*://*.schoology.com/*"],
		"scripts" : ["background.js"]
	},
	"permissions": [
    	"tabs",
		"storage",
		"*://*.schoology.com/*"
  	],
	"browser_action": {
		"default_popup": "popup.html",
		"default_title": "Test"
	}
}
